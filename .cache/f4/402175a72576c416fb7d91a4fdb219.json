 }\n\n  this.message = message;\n  this.type = errorType;\n  this.statusCode = options.statusCode || null;\n  this.request = options.request;\n  this.body = body;\n}\n\nmodule.exports = MapiError;\n"},"lineCount":null}},"error":null,"hash":"e16c8b7fa57e39e40b5a6321e33921fa","cacheData":{"env":{}}}